        <record id="product_template_form_view_inherited_th" model="ir.ui.view">
            <field name="name">product.template.tree.stock.tree</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <field name="type" position="after">
                    <field name="last_move_date"/>
                </field>
            </field>
        </record>


        <record id="product_template_tree_stock_view_2" model="ir.ui.view">
            <field name="name">product.template.tree.stock.tree</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="last_move_date"/>
                </field>
            </field>
        </record>

        <record id="product_product_tree_view_inherited_th" model="ir.ui.view">
            <field name="name">product.product.tree.stock.tree</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="last_move_date"/>
                </field>
            </field>
        </record>


        <record id="product_template_tree_stock_view" model="ir.ui.view">
            <field name="name">product.template.tree.stock.tree</field>
            <field name="model">product.template</field>
            <field name="arch" type="xml">
                <tree string="Product">
                    <!--<field name="sequence" widget="handle"/>-->
                    <!--<field name="default_code"/>-->
                    <!--<field name="name"/>-->
                    <!--<field name="list_price"/>-->
                    <field name="standard_price"/>
                    <field name="categ_id"/>
                    <field name="type"/>
                    <field name="uom_id" options="{'no_open': True, 'no_create': True}" groups="product.group_uom"/>
                    <field name="active" invisible="1"/>
                </tree>
            </field>
        </record>


